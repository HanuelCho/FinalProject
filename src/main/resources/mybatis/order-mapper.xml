<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="OrderDAO">

	<insert id="insert">
		insert into products_order
		values(order_seq.nextval,#{products_seq},#{seller_id},#{buyer_id},default,#{products_title},#{products_location},#{products_price},#{products_quantity},#{products_imagepath})
	</insert>
</mapper>