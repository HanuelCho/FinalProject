<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.shopboard-mapper">



<insert id="ShopBoardInsert">
 insert into shopboard values(SHOPBOARD_SEQ.NEXTVAL,#{shop_id},#{shop_title},#{shop_contents},default,#{shop_location},#{shop_expiration},#{shop_price},#{shop_quantity},#{shop_imagepath})
</insert>



<select id="ShopBoardSelect" resultType="com.project.dto.ShopBoardDTO">
select * from shopboard
</select>

 
 
<select id="ShopBoardIdSelect" resultType="com.project.dto.ShopBoardDTO">
select * from shopboard where shop_seq=#{value}
</select>
 
 
 
 
 
</mapper>